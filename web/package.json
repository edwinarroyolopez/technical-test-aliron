    "build": "npm run build-css && react-scripts build",
    "build:test": "npm run build-css && cross-env REACT_APP_ENV='test' react-scripts build",
    "build:prod": "npm run build-css && cross-env REACT_APP_ENV='prod' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tslint -c ./tslint.json './src/**/*.ts'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
