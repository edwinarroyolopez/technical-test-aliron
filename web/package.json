    "start-js": "react-scripts start",
    "start:prod": "PORT=3000 cross-env REACT_APP_ENV='prod' npm-run-all -p watch-css start-js",
    "start:test": "PORT=3000 cross-env REACT_APP_ENV='test' npm-run-all -p watch-css start-js",
    "start": "PORT=3000 cross-env REACT_APP_ENV='dev' npm-run-all -p watch-css start-js",
